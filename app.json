{
  "configVersion": "v3",
  "app": {
    "appId": 1056908,
    "appName": "Smart Timers",
    "appType": "app",
    "version": {
      "code": 4,
      "name": "4.0"
    },
    "icon": "iconApp.png",
    "vender": "Albert Safiullin albertincx@gmail.com",
    "description": "An app creates timers with alarm"
  },
  "permissions": [
    "device:os.notification",
    "data:os.device.info",
    "device:os.alarm",
    "device:os.local_storage"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "4.0",
      "target": "4.2",
      "minVersion": "4.0"
    }
  },
  "targets": {
    "common": {
      "module": {
        "page": {
          "pages": [
            "pages/alarm",
            "pages/alarm-popup",
            "pages/creation-wizard",
            "pages/vibration-settings"
          ]
        },
        "app-service": {
          "services": ["app-service/index"]
        }
      },
      "platforms": [
        {
          "st": "r"
        }
      ],
      "designWidth": 480
    }
  },
  "debug": false,
  "defaultLanguage": "en-US"
}
